{
  "name": "uke-dashboard",
  "version": "1.0.0-rc.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "npm run setAppVersion",
    "start": "PORT=6086 PLATFORM=workspace_admin ukectl start",
    "build": "npm run setAppVersion; npm run build:uke;",
    "copy:version": "cp src/version.json build/",
    "build:uke": "ukectl build; npm run copy:version",
    "deploy": "npm run build",
    "initWrokspace": "node ./scripts/init-workspace",
    "init": "git init; npm i; npm run setAppVersion",
    "setAppVersion": "vh gen",
    "injectVersionToTmpl": "node ./version/inject-version-to-tmpl.js",
    "autoZip": "npm run build; npm run zipBuild;",
    "prettier:base": "prettier --parser typescript",
    "format": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "update": "yarn add uke-request uke-admin-web-scaffold ukelli-ui -W; yarn start",
    "zipBuild": "mkdir -p ./zip-build; zip -r ./zip-build/build.zip ./build/*"
  },
  "dependencies": {
    "basic-helper": "^3.0.7",
    "mousetrap": "^1.6.2",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.6",
    "react-transition-group": "^4.2.1",
    "uke-admin-web-scaffold": "^1.2.0-rc1",
    "uke-request": "^1.1.0-rc5",
    "ukelli-ui": "^3.1.0-rc14",
    "unistore": "^3.1.0",
    "version-helper": "^1.0.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "prettier": "^1.18.2",
    "uke-scripts": "^1.1.0-alpha3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
