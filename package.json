{
  "name": "uke-dashboard",
  "version": "0.6.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "PORT=6086 PLATFORM=workspace_admin ukectl start",
    "build": "npm run setAppVersion; ukectl build",
    "deploy": "PLATFORM=netlify npm run build",
    "initWrokspace": "node ./scripts/init-workspace",
    "init": "git init; npm i; npm run setAppVersion",
    "setAppVersion": "sh ./version/set-app-version.sh",
    "injectVersionToTmpl": "node ./version/inject-version-to-tmpl.js",
    "autoZip": "npm run build; npm run zipBuild;",
    "update": "yarn add uke-request uke-admin-web-scaffold ukelli-ui -W; yarn start",
    "zipBuild": "mkdir -p ./zip-build; zip -r ./zip-build/build.zip ./build/*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "basic-helper": "^3.0.6",
    "mousetrap": "^1.6.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.6",
    "react-transition-group": "^4.2.1",
    "uke-admin-web-scaffold": "^1.0.0-rc.3",
    "uke-request": "^1.0.5",
    "ukelli-ui": "^3.0.0-rc.5",
    "unistore": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.12.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "uke-scripts": "^0.4.19"
  }
}
