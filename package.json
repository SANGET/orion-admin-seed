{
  "name": "uke-dashboard",
  "version": "1.0.0-rc.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "PORT=6086 PLATFORM=workspace_admin ukectl start",
    "build": "npm run setAppVersion; ukectl build",
    "deploy": "npm run build",
    "initWrokspace": "node ./scripts/init-workspace",
    "init": "git init; npm i; npm run setAppVersion",
    "setAppVersion": "sh ./version/set-app-version.sh",
    "injectVersionToTmpl": "node ./version/inject-version-to-tmpl.js",
    "autoZip": "npm run build; npm run zipBuild;",
    "update": "yarn add uke-request uke-admin-web-scaffold ukelli-ui -W; yarn start",
    "zipBuild": "mkdir -p ./zip-build; zip -r ./zip-build/build.zip ./build/*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "basic-helper": "^3.0.6",
    "mousetrap": "^1.6.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.6",
    "react-transition-group": "^4.2.1",
    "uke-admin-web-scaffold": "^1.0.0-rc.9",
    "uke-request": "^1.0.8",
    "ukelli-ui": "^3.0.0-rc.17",
    "unistore": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "uke-scripts": "^1.0.0-rc.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
